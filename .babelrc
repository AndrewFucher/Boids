{
  "presets": [
    "@babel/env",
    [
      "@babel/react",
      { "runtime": "automatic", "importSource": "@emotion/react" }
    ],
    "@babel/preset-typescript",
    [
      "@emotion/babel-preset-css-prop",
      {
        "sourceMap": true,
        "autoLabel": "dev-only",
        "labelFormat": "[local]",
        "cssPropOptimization": true
      }
    ]
    // "@babel/preset-react"
  ],
  "plugins": ["@babel/plugin-proposal-class-properties", ["@emotion/babel-plugin"]]
}
